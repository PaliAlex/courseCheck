    "@babel/plugin-transform-template-literals": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "babel-loader": "^8.2.2",
    "jest": "^29.6.2",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0"
  },
